    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^4.19.1",
    "eslint-config-fullstack": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
