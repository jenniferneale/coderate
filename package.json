    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-session-sequelize": "^4.1.0",
    "express": "^4.16.3",
    "express-session": "^1.15.1",
    "history": "^4.6.3",
    "morgan": "^1.9.1",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^3.6.0",
