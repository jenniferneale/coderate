{
  "name": "coderate",
  "version": "1.0.0",
  "description": "App for benchmarking an engineer's coding and communication skills",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "start webpack -w --display-error-details",
    "deploy": "script/deploy",
    "lint": "eslint ./ --ignore-path .gitignore",
    "seed": "node script/seed.js",
    "start": "node server",
    "start-dev": "npm run build-client-watch & npm run start-server",
    "start-server": "start nodemon server -e html,js,scss --ignore public",
    "test": "mocha ./server/**/*.spec.js ./server/**/**/*.spec.js ./client/**/*.spec.js --compilers js:babel-register"
  },
  "author": "",
  "license": "ISC",
